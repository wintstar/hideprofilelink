{
    "name": "wintstar/hideprofilelink",
    "type": "phpbb-extension",
    "description": "Adds permissions to the link in the user profile and not view profile.",
    "homepage": "https://www.stephan-frank.de/",
    "version": "1.0.6",
    "keywords": [
        "phpbb",
        "extension",
        "profile"
    ],
    "license": "GPL-2.0-only",
	"authors": [{
			"name": "Stephan-Frank",
			"homepage": "https://www.stephan-frank.de",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.0.0,<=8.4.0",
		"composer/installers": "~1.0.0"
		},
	"extra": {
		"display-name": "Hide user profile link",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<3.4.0@dev"
		},
		"version-check": {
			"host": "stephan-frank.de",
			"directory": "/vc",
			"filename": "hideprofilelink.json",
			"ssl": true
		}
	}
}
